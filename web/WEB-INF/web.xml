<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>Sign</servlet-name>
        <servlet-class>Sign</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sign</servlet-name>
        <url-pattern>/sign</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Reg</servlet-name>
        <servlet-class>Reg</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Reg</servlet-name>
        <url-pattern>/reg</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SetTwit</servlet-name>
        <servlet-class>WriteTwit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SetTwit</servlet-name>
        <url-pattern>/set_twit</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteTwit</servlet-name>
        <servlet-class>DeleteTwit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteTwit</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AllUsers</servlet-name>
        <servlet-class>AllUsers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AllUsers</servlet-name>
        <url-pattern>/all_users</url-pattern>
    </servlet-mapping>
</web-app>